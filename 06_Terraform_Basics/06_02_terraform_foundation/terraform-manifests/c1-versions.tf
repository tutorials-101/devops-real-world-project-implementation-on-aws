# Terraform Block
terraform {
  required_version = ">= 1.0.0" 
  required_providers {
    aws = {
      source = "hashicorp/aws"
      version = "~> 6.0"
    }
    random = {
      source = "hashicorp/random"
      version = "~> 3.0"
    }    
  }
}

# Provider Block
provider "aws" {
  region = "us-east-1"
}



